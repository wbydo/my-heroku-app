  "scripts": {
    "test": "jest",
    "serverside:ts-node": "ts-node -r tsconfig-paths/register --project ./serverside/tsconfig.json",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev:serve:serverside": "NODE_ENV=macbook TS_NODE_PROJECT=./serverside/tsconfig.json nodemon --exec \"ts-node -r tsconfig-paths/register ./serverside/src/server.ts\" --watch ./serverside/src --ext ts",
    "dev:serve:frontend": "NODE_ENV=macbook TS_NODE_PROJECT=./frontend/tsconfig.json webpack-dev-server --config ./frontend/webpack/serve.ts",
    "dev:serve": "concurrently \"npm run dev:serve:serverside\" \"npm run dev:serve:frontend\"",
    "typeorm:local": "npm run serverside:ts-node -- ./node_modules/typeorm/cli.js --config=./serverside/src/db/ormconfig.ts",
    "typeorm:local:entity:create": "npm run typeorm:local -- entity:create",
    "typeorm:local:migration:generate": "npm run typeorm:local -- migration:generate",
    "typeorm:local:migration:run": "npm run typeorm:local -- migration:run",
    "typeorm:local:migration:revert": "npm run typeorm:local -- migration:revert",
    "typeorm:hoge": "npm run serverside:ts-node -- ./node_modules/typeorm/cli.js --config=./serverside/src/db/ormconfig.ts",
    "typeorm:hoge:entity:create": "npm run typeorm -- entity:create",
    "typeorm:hoge:migration:generate": "npm run typeorm -- migration:generate",
    "typeorm:hoge:migration:run": "npm run typeorm -- migration:run",
    "typeorm:hoge:migration:revert": "npm run typeorm -- migration:revert"
